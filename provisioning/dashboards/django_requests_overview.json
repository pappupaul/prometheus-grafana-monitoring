      "id": 2,
      "options": {
        "reduceOptions": {
          "calcs": [
          ]
        }
      },
      "targets": [
        {
          "datasource": {
            "uid": "prometheus"
          },
          "expr": "round(\n  sum(\n    rate(\n      django_http_requests_total_by_view_transport_method_total{\n        view!~\"<unnamed view>|health_check:health_check_home|prometheus-django-metrics\",\n      }[$__rate_interval]\n    )\n  ), 0.001\n)\n"
        }
      ],
      "title": "Request Volume",
      "type": "stat"
    }
  ],
